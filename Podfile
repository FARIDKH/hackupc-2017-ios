platform :ios, '9.0'

target 'HNotes-ios' do
  use_frameworks!

  # Pods for HNotes-ios

  #Cache image
  pod 'Kingfisher'

  #Camera
  pod 'ImagePicker'

  #DB
  pod 'RealmSwift'

  #HTTP request handling
  pod 'Alamofire'

  #Facebook sdk
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

  #json parsing
  pod 'SwiftyJSON'
  
  #popup
  pod 'SwiftMessages'
  
  #Google login
  pod 'GoogleSignIn'
  
end
